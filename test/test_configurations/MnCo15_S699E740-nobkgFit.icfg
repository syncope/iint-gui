despike:
  input: intensity
  method: p09despiking
  output: despikedIntensity
  type: filter1d
execlist:
- specread
- observabledef
- despike
- trapint
- signalcurvefit
observabledef:
  detector_column: exp_c01
  exposureTime_column: exp_t01
  id: scannumber
  input: rawdata
  monitor_column: sumvfcs_counts
  motor_column: pth
  output: intensity
  type: iintdefinition
signalcurvefit:
  error: None
  model:
    m0_:
      m0_amplitude:
        value: 2.0
      m0_center:
        value: 1.0
      m0_fwhm:
        value: 21.0
      m0_height:
        value: 22.0
      m0_sigma:
        value: 3.0
      modeltype: gaussianModel
  result: signalcurvefitresult
  type: curvefitting
  useguessing: 1
  usepreviousresult: 0
  xdata: pth
  ydata: intensity
specread:
  filename: /home/rosem/iintgui/test/test_data/MnCo15.spc
  output: default
  scanlist: 699-740
  type: specfilereader
trapint:
  motor: pth
  observable: signalIntensity
  output: trapezoidIntegral
  type: trapezoidintegration
